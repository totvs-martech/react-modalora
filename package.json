{
  "name": "react-modalora",
  "version": "0.0.0",
  "private": true,
  "description": "Modal for React 16+",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/totvs-store/react-modalora.git"
  },
  "bugs": {
    "url": "https://github.com/totvs-store/react-modalora/issues"
  },
  "homepage": "https://github.com/totvs-store/react-modalora#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.6.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "react-dom": "^16.5.2"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.5.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "build:babel": "babel -d dist -s --ignore *.test.js src",
    "build:copy-readme": "cp README.md dist",
    "build:create-dist": "mkdir -p dist",
    "build:generate-package": "node build/generate-package.js",
    "build": "$npm_execpath run build:create-dist; $npm_execpath run build:generate-package; $npm_execpath run build:babel; $npm_execpath run build:copy-readme",
    "lint": "eslint src",
    "test": "jest",
    "watch": "$npm_execpath run build:babel -w"
  }
}
